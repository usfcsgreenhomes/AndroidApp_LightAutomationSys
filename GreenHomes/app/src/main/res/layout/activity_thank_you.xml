<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.usfca.greenhomes.ThankYou"
    android:background="#FFE8F0DD">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Thanks for reaching out to us!!"
        android:id="@+id/contactThanks"
        android:textColor="#012807"
        android:textAlignment="gravity"
        android:layout_below="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:typeface="serif" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:src="@drawable/abc_btn_check_to_on_mtrl_015"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="We will get back to you shortly."
        android:id="@+id/textView6"
        android:textColor="#012807"
        android:textAlignment="gravity"
        android:typeface="serif"
        android:layout_below="@+id/contactThanks"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
